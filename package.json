{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint ./src/**/*.{js,jsx} App.js --no-error-on-unmatched-pattern",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@expo/ngrok": "^4.1.0",
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-picker/picker": "1.9.2",
    "@unimodules/react-native-adapter": "^6.3.7",
    "date-fns": "^2.24.0",
    "dotenv": "^10.0.0",
    "expo": "~40.0.0",
    "expo-asset": "^8.3.3",
    "expo-file-system": "^11.1.3",
    "expo-font": "^9.2.1",
    "expo-linking": "^2.3.1",
    "expo-status-bar": "~1.0.3",
    "formik": "^2.2.9",
    "graphql": "^15.5.2",
    "number-abbreviate": "^2.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-element-dropdown": "^2.12.0",
    "react-native-elements": "^3.4.3",
    "react-native-paper": "^5.12.3",
    "react-native-web": "~0.13.12",
    "react-router-native": "^6.0.0-beta.8",
    "use-debounce": "^10.0.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.15.0",
    "@expo/webpack-config": "^0.15.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^26.6.3",
    "jest-expo": "^42.1.0"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  }
}
